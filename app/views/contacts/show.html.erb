<%- model_class = Contact -%>
<!-- <div class="page-header">
  <h1><%=t '.title', default: model_class.model_name.human.titleize %></h1>
</div> -->
<div class="form-group">
  <%= link_to t('.back', default: t("helpers.links.back")),
              :back, class: 'btn btn-default'  %>
</div>
<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:name) %></dt>
      <dd><pre class="prettyprint"><%= @contact.name %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:line1) %></dt>
      <dd><pre class="prettyprint"><%= @contact.line1 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:line2) %></dt>
      <dd><pre class="prettyprint"><%= @contact.line2 %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:city) %></dt>
      <dd><pre class="prettyprint"><%= @contact.city %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:state) %></dt>
      <dd><pre class="prettyprint"><%= @contact.state %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:zip) %></dt>
      <dd><pre class="prettyprint"><%= @contact.zip %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:phone) %></dt>
      <dd><pre class="prettyprint"><%= @contact.phone %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', default: t("helpers.links.back")),
              :back, class: 'btn btn-default'  %>
  <!-- <%= link_to t('.edit', default: t("helpers.links.edit")),
              edit_contact_path(@contact), class: 'btn btn-default' %>
  <%= link_to t('.destroy', default: t("helpers.links.destroy")),
              contact_path(@contact),
              method: 'delete',
              data: { confirm: t('.confirm', default: t("helpers.links.confirm", default: 'Are you sure?')) },
              class: 'btn btn-danger' %> -->
</div>
